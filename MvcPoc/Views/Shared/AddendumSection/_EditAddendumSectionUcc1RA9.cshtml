@model MvcPoc.Web.Models.Addendum.Ucc1AddendumModel

<script src="~/Scripts/jquery-1.8.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

<style type="text/css">
        .Addendum-section {
            border: 2px solid #030303;
            padding-left: 20px;
        }

        .Addendum-section1 {
            border: 2px solid grey;
            margin: 20px 20px 20px 0px;
        }

       
        .section-header h6 {
            background-color: grey;
            color: black;
            margin: 0;
            padding: 5px;
        }

        input[type="radio"] {
            width: 20px;
        }
    </style>   
<div class="Addendum-section">
    <div class="Addendum-section1">
        <div class="section-header">
            <h6>Addendum</h6>
            <fieldset>
                <legend>Ucc1AddendumModel</legend>

                <div class="editor-label">
                    @Html.LabelFor(model => model.FiledinRealEstate)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.FiledinRealEstate)
                    @Html.ValidationMessageFor(model => model.FiledinRealEstate)
                </div>

                <div class="editor-label">
                    @Html.LabelFor(model => model.Addendumfinancingstmt)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.Addendumfinancingstmt)
                    @Html.ValidationMessageFor(model => model.Addendumfinancingstmt)
                </div>

                <div class="editor-label">
                    @Html.LabelFor(model => model.NameAddressofrecordowner)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.NameAddressofrecordowner)
                    @Html.ValidationMessageFor(model => model.NameAddressofrecordowner)
                </div>

                <div class="editor-label">
                    @Html.LabelFor(model => model.DescriptionofRealEstate)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.DescriptionofRealEstate)
                    @Html.ValidationMessageFor(model => model.DescriptionofRealEstate)
                </div>

                <div class="editor-label">
                    @Html.LabelFor(model => model.AddAttachment)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.AddAttachment)
                    @Html.ValidationMessageFor(model => model.AddAttachment)
                    <p>
                        <input type="submit" value="Browse"/>
                    </p>
                </div>

                <div class="editor-label">
                    @Html.LabelFor(model => model.AttachmentDescription)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.AttachmentDescription)
                    @Html.ValidationMessageFor(model => model.AttachmentDescription)
                </div>

                <div class="editor-label">
                    @Html.LabelFor(model => model.Miscellaneous)
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.Miscellaneous)
                    @Html.ValidationMessageFor(model => model.Miscellaneous)
                </div>
                 
            </fieldset>
        </div>
    </div>
</div>



