    <style type="text/css">
        .Addendum-section {
            border: 2px solid #030303;
            padding-left: 20px;
        }

        .Addendum-section1 {
            border: 2px solid grey;
            margin: 20px 20px 20px 0px;
        }

       
        .section-header h6 {
            background-color: grey;
            color: black;
            margin: 0;
            padding: 5px;
        }

